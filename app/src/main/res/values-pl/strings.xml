<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:xliff="http://schemas.android.com/tools">

    <!--
         Used in Action Bar, and in AndroidManifest to tell the device the name of this app.
         It's to keep this short, so your launcher icon isn't displayed with "The greatest Wea"
         or something similar.
    -->

    <string name="title_activity_detail">Szczegóły</string>
    <string name="title_activity_settings">Ustawienia</string>

    <!--
         By convention, "action" denotes that this String will be used as the label for an Action,
         typically from the action bar.  The ActionBar is limited real estate, so shorter is better.
    -->
    <string name="action_settings">Ustawienia</string>
    <string name="action_map">Znajdź na mapie</string>
    <string name="action_share">Udostępnij</string>

    <!-- Menu label to fetch updated weather info from the server -->
    <string name="action_refresh">Odśwież</string>

    <!-- Label for the location preference [CHAR LIMIT=30] -->
    <string name="pref_location_label">Lokalizacja</string>


    <!-- Label for the temperature units preference [CHAR LIMIT=30] -->
    <string name="pref_units_label">Jednostka</string>

    <!-- Label for metric option in temperature unit preference [CHAR LIMIT=25] -->
    <string name="pref_units_label_metric">Metryczne</string>

    <!-- Label for imperial option in temperature unit preference [CHAR LIMIT=25] -->
    <string name="pref_units_label_imperial">Imperialne</string>

    <!-- Date label when displaying today's weather forecast [CHAR LIMIT=20] -->
    <string name="today">Dziś</string>

    <!-- Date label when displaying tomorrow's weather forecast [CHAR LIMIT=20] -->
    <string name="tomorrow">Jutro</string>

    <!-- Date format for displaying day of week and date (i.e. Mon Jun 1) [CHAR LIMIT=20] -->
    <string name="format_full_friendly_date"><xliff:g id="month">%1$s</xliff:g>, <xliff:g id="day">%2$s</xliff:g></string>

    <!-- Strings for formatting weather-related data -->
    <!-- Temperature format [CHAR LIMIT=5] -->
    <string name="format_temperature"><xliff:g id="temp">%1.0f</xliff:g>\u00B0</string>

    <!-- Windspeed formats -->
    <!-- Wind in mph [CHAR LIMIT=25] -->
<string name="format_wind_mph">Wiatr: <xliff:g id="speed">%1$1.0f</xliff:g> mph <xliff:g id="direction">%2$s</xliff:g>
</string>

    <!-- Wind in kph [CHAR LIMIT=25] -->
<string name="format_wind_kmh">Wiatr: <xliff:g id="speed">%1$1.0f</xliff:g> km/h <xliff:g id="direction">%2$s</xliff:g>
</string>

    <!-- Pressure format CHAR LIMIT=25] -->
    <string name="format_pressure">Ciśnienie: <xliff:g id="pressure">%1.0f</xliff:g> hPa</string>

    <!-- Humidity format CHAR LIMIT=25]-->
    <string name="format_humidity">Wilgotność: <xliff:g id="humidity">%1.0f</xliff:g> %%</string>
    <!-- Account type (for sync-adapter authenticator) -->
    <!-- SyncAdapter related -->
    <string name="sync_account_type">sunshine.example.com</string>

    <!-- Notification Format -->
    <string name="format_notification">
    Forecast: <xliff:g id="condition">%1$s</xliff:g> Maks: <xliff:g id="high">%2$s</xliff:g> Min: <xliff:g id="low">%3$s</xliff:g>
    </string>
    <!-- Strings related to Notification preference -->
    <string name="pref_last_notification">last_notification
    </string>
    <string name="notification_title">Powiadomienia Sunshine</string>

    <!-- Strings related to Notification Enabled preference -->

    <string name="pref_enable_notifications_label">Powiadomienia pogodowe</string>
    <string name="pref_enable_notifications_true">Włączone</string>
    <string name="pref_enable_notifications_false">Wyłączone</string>


    <string name="weather_empty">No weather information available</string>
    <string name="no_network">Brak informacji o pogodzie. Sprawdź połączenie z siecią.</string>


    <string name="empty_forecast_list_server_down">Brak informacji o pogodzie. Serwer nie zwraca danych.</string>
    <string name="empty_forecast_list_server_error">Brak informacji o pogodzie. Serwer zwraca nieprawidłowe dane. Spróbuj uaktualnić Sunshine</string>
    <string name="empty_forecast_list_invalid_location">Brak informacji o pogodzie. Nie znaleziono lokalizacji.</string>
    <!-- Strings used when displaying the state of the Location in settings -->
    <string name="pref_location_error_description">Nieprawidłowa lokalizacja (<xliff:g id="location_setting">%1$s</xliff:g>)"</string>
    <string name="pref_location_unknown_description">Sprawdzanie lokalizacji.. (<xliff:g id="location_setting">%1$s</xliff:g>)"</string>
    <string name="maximum_temperature">Temperatura maksymalna: <xliff:g id="high">%1$s</xliff:g></string>
    <string name="minimum_temperature">Temperatura minimalna: <xliff:g id="low">%1$s</xliff:g></string>
    <string name="location_edit_hint">Wprowadź lokalizację aby pobrać pogodę.</string>

    <string name="Snow">Śnieg</string>
